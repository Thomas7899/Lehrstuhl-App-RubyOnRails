<p style="color: green">
  <%= notice %>
</p>

<% content_for :title, "Abstrakte Abschlussarbeiten" %>

<div id="search_results" class="text-center">
    
</div>

  <h1 class="text-center mb-4">Abstrakte Abschlussarbeiten</h1>

  <div class="text-center">
    <%= link_to "Abschlussarbeit erstellen" , new_abstrakte_abschlussarbeit_path, class: "btn btn-success" %>
  </div>

  <div id="abstrakte_abschlussarbeits" class="container mt-4">
    <div class="row">
      <% @abstrakte_abschlussarbeits.each do |abstrakte_abschlussarbeit| %>
        <div class="col-md-6 col-lg-4 mb-4">
          <div class="card">
            <div class="card-body">
              <%= render abstrakte_abschlussarbeit %>
                <p class="mt-3">
                  <%= link_to "Show" , abstrakte_abschlussarbeit, class: "btn btn-primary btn-sm" %>
                </p>
            </div>
          </div>
        </div>
        <% end %>
    </div>

    <div class="d-flex justify-content-center">
      <div class="digg_pagination">
        <%= will_paginate @abstrakte_abschlussarbeits, :container => false %>
      </div>
    </div>
    